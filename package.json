{
  "name": "front_end",
  "version": "*",
  "private": true,
  "homepage": "http://lukeshay.com",
  "main": "src/index.tsx",
  "author": "Luke Shay",
  "license": "LGPL",
  "scripts": {
    "start": "recaptchaKey=$GOOGLE_API_KEY webpack-dev-server --open --hot --config configs/webpack.dev.js",
    "start:local": "recaptchaKey=$GOOGLE_API_KEY webpack-dev-server --open --hot --config configs/webpack.local.js",
    "build": "recaptchaKey=$GOOGLE_API_KEY webpack -p --config=configs/webpack.prod.js",
    "deploy": "serve -p 80 -s ./dist",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "format": "yarn lint --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "yarn typecheck && yarn lint",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/react-router-dom": "*",
    "material-ui": "*",
    "react": "*",
    "react-dom": "*",
    "react-google-recaptcha": "*",
    "react-inlinesvg": "*",
    "react-recaptcha-google": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-scripts": "*",
    "react-toastify": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/preset-typescript": "*",
    "@loadable/component": "*",
    "@lukeshay/eslint-config-react-typescript": "*",
    "@svgr/webpack": "*",
    "@testing-library/react": "*",
    "@types/jest": "*",
    "@types/universal-cookie": "*",
    "babel-jest": "*",
    "babel-loader": "*",
    "brotli-webpack-plugin": "*",
    "clean-webpack-plugin": "*",
    "css-loader": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "enzyme-to-json": "*",
    "eslint": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "image-webpack-loader": "*",
    "jest": "*",
    "jest-junit": "*",
    "node": "*",
    "react-hot-loader": "*",
    "react-transition-group": "*",
    "source-map-loader": "*",
    "style-loader": "*",
    "ts-jest": "*",
    "ts-loader": "*",
    "typescript": "*",
    "typings-for-css-modules-loader": "*",
    "universal-cookie": "*",
    "webpack-cli": "*",
    "webpack-dev-middleware": "*",
    "webpack-dev-server": "*",
    "webpack-merge": "*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest-junit": {
    "output": "./reports/jest-junit.xml",
    "ancestorSeparator": " , ",
    "classNameTemplate": ""
  }
}
